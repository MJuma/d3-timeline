{
  "name": "@mjuma/timeline",
  "version": "1.0.0",
  "description": "D3 timeline",
  "keywords": [
    "d3",
    "timeline",
    "time-series"
  ],
  "author": {
    "name": "M Juma",
    "url": "https://github.com/MJuma"
  },
  "license": "MIT",
  "homepage": "https://github.com/MJuma/d3-timeline",
  "repository": {
    "type": "git",
    "url": "https://github.com/MJuma/d3-timeline.git"
  },
  "main": "dist/timeline.cjs.js",
  "module": "dist/timeline.js",
  "typings": "dist/timeline.d.ts",
  "scripts": {
    "clean": "rimraf dist/*",
    "build": "npm run clean && rollup --config rollup.config.ts --configPlugin typescript",
    "watch": "npm run build -- --watch",
    "typecheck": "tsc"
  },
  "engines": {
    "node": ">=16"
  },
  "files": [
    "CHANGELOG.md",
    "dist/**/*"
  ],
  "dependencies": {
    "d3-array": "3.1.6",
    "d3-axis": "3.0.0",
    "d3-brush": "3.0.0",
    "d3-scale": "4.0.2",
    "d3-scale-chromatic": "3.0.0",
    "d3-selection": "3.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "13.3.0",
    "@rollup/plugin-typescript": "8.3.2",
    "@types/d3": "7.4.0",
    "rimraf": "3.0.2",
    "rollup": "2.75.6",
    "rollup-plugin-dts": "4.2.2",
    "rollup-plugin-esbuild": "4.9.1",
    "rollup-plugin-visualizer": "^5.6.0",
    "tslib": "2.4.0",
    "typescript": "4.7.3"
  }
}
